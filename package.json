{
  "name": "jig",
  "description": "my frontend-product template",
  "version": "0.0.1",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "cpx": "^1.3.1",
    "cross-env": "^1.0.7",
    "fixpack": "^2.3.0",
    "globify": "^1.2.1",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^1.7.0"
  },
  "keywords": [],
  "license": "WTFPL",
  "main": "index.js",
  "private": true,
  "scripts": {
    "_build:js": "globify \"src/**/*.js\" -u \"**/_*.js\" -o \"public/assets\"",
    "_mkdirp:assets": "mkdirp public/assets",
    "_serve": "http-server -o",
    "_watch:js": "globify \"src/**/*.js\" -u \"**/_*.js\" -o \"public/assets\" -w -d -v",
    "fixpack": "fixpack",
    "start": "npm-run-all _mkdirp:assets -p _watch:* _serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
